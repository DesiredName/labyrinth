{
  "name": "vite-npm-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w @webx/web\" \"npm run dev -w @webx/ui\" \"npm run dev -w @webx/server\"",
    "build": "npm run build --workspaces",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "c": "git add . && git commit -m \"update\" && git push"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/styled-components": "^5.1.36",
    "@types/react-dom": "^19.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react-router": "^5.1.20",
    "@tailwindcss/vite": "^4.1.17",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite-plugin-dts": "^4.5.4",
    "@vitejs/plugin-react": "^5.1.1"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "cookie-parser": "^1.4.7",
    "framer-motion": "^12.23.24",
    "cors": "^2.8.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "mssql": "^12.1.1",
    "es-cookie": "^1.5.0",
    "styled-components": "^6.1.19",
    "tailwindcss": "^4.1.17",
    "tailwind-merge": "^3.4.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.1.13",
    "sequelize": "^6.37.7",
    "express": "^5.1.0",
    "vite": "^7.2.4"
  }
}